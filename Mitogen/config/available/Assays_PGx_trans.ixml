# stepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstep

    step PGx Load Quant Studio
      form
        include userAccountInfo
        include resourceTable
        style
          type= text/css
        style
          - [barcode=barcode] {
          - background-image: url(images/barcodebkg.jpg);
          - }
        script 
          - function showQuantRack() {
          -   $('#quantPlate').load('/uniflow', {stepName: 'Ajax Quant Studio Search Server',
          -     pcrPlate: $('#pcrPlate').val()
          -   });
          -   $('#quantPlate').dialog({ minWidth: 500 });
          - }
          - $(document).ready(function() {
          - $('#showContents').click(function(){
          -    showQuantRack();
          -  }); });
#       queueQuery 
#         - SELECT q.containerId AS "Extract" ,
#         - c.content AS "Comments" ,
#         - e.eventDate, e.userId
#         - FROM queues q
#         - INNER JOIN events e
#         - ON q.eventId = e.eventId
#         - LEFT JOIN contents c
#         - ON q.containerid = c.containerId
#         - AND q.eventId = c.eventId
#         - AND 'Notes' = c.contentType
#         - WHERE q.step = 'PGx Load Quant Studio'
        vertical
          fieldset
            legend Directions
              class= legend
            vertical
              li Scan the ID of the <b> plate </b> being loaded.
              li Click the <b>Show Plate Contents</b> button to view samples to be added to Quant Studio. 
              li When samples are added they will be queued for <b> Translational Data Upload </b>

        vertical2
          container pcrPlate
            id= pcrPlate
            style= background-image: url(images/barcodebkg.jpg);
            screenLabel~ Plate ID
            acceptQueue~ any
            barcode= barcode 
            deleteQueue~ {_step}
            sql~ insert into queues select content, 'Translational Data Upload', {_eventId} from contents where containerId = '{pcrPlate}' and contentType = 'aliquotId'
          button Show Plate Contents
            id= showContents
        vertical
          div
            id= quantPlate

