config
  steps
    stepGroup PGx Translational

# stepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstep
    step PGx Lysis
# stepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstep
      form
        
        include userAccountInfo
        include resourceTable
        style
          type= text/css
        style
          - [barcode=barcode] {
          - background-image: url(images/barcodebkg.jpg);
          - }
        vertical
          fieldset
            legend Directions
              class= legend
            vertical
              li Select the <b>Resources</b> used for this step in the <b>Resources</b> tab.
              li Enter <b>Specimen Id (swab)</b> and the <b>Lysis Basket</b>.
              li Only <b>Specimen Ids (swabs)</b> queued for this step will be available for lysis.
        vertical
          table
            class= stdTableSort
            sqlQuery~ 
              - SELECT  
              -   '' AS "Specimen" ,
              -   '' AS "Extract" 
#              -   '' AS "Comments"
              - FROM queues
              - WHERE step = '{_step}'
              - LIMIT 24
            columnSpecs~ lysis
              allowChangedRecordIds
              column 1
                inputType container
                  barcode= barcode
                  containerType~ oncoDxId
                  required~ false
                  if~ {_columnInput:2}_blank <> _blank
                    deleteQueue~ {_step}
              column 2
                inputType container
                  containerType~ lysisTubeId
                  barcode= barcode
                  required~ false
                  new~ true
                  addContent~
                  insertQueue~ PGx Extraction Prep
                  if~ {_thisInput}_BLANK <> _BLANK
                    sql INSERT INTO contents (containerId, attribute, contentType, content, eventId)
                      - (SELECT '{_columnInput:2}', c.attribute, c.contentType, c.content, {_eventId}
                      - FROM contents c
                      - WHERE c.containerId = '{_columnInput:1}')

# stepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstep

    step PGx Extraction Prep
# stepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstepstep
      form
        script
          - $(document).ready(function() { 
          -   $('[name^="QiaSymphony Elution"]').css('background-image', 'url(images/barcodebkg.jpg)');
          - });
        include userAccountInfo
        include resourceTable
        vertical
          fieldset
            legend Directions
              class= legend
            vertical
              li Scan barcodes for <b>Extraction</b> and <b>Elution</b> racks.
              li Scan in QIAsymphony <b>Extraction</b> tubes into the <b>1x24</b> grid.
              li Scan in QIAsymphony <b>Elution</b> tubes in the <b>6x4</b> grid.
              li The fields tab <b>DOWN </b>the Elution rack.
        div
          div
            style= float:left;
            container sampleRack
              new~ true
              screenLabel~ 
              value= SR{_getNextSequence:sampleRack}
              addContent~
              readOnly=
              style= display: none;
            grid QIAsymphony Sample Rack
              cols 1
              rows 24
              addContentsTo sampleRack
                contentType lysisTubeId
              inputType container
                containerType~ lysisTubeId
                deleteQueue~ {_step}
                required~ false
                size= 10
                style= background-image: url(images/barcodebkg.jpg);
          div
            style= float:left;width:40px;
            span &nbsp;&nbsp;&nbsp;
          div
            style= float:left;
            container elutionRack
              new~ true
              screenLabel~ 
              value= ER{_getNextSequence:elutionRack}
              addContent~
              readOnly=
              style= display: none;
            plate QIAsymphony Elution Rack
              tabIndexColumnMajor true
              cols 6
              rows 4
              addContentsTo elutionRack
                contentType elutionTubeId
              inputType container
                new~ true
                addContent~ 
                required~ false
                size= 5
                containerType~ elutionTubeId
                deleteQueue~ {_step}
                insertQueue~ PGx Load QIAgility Rack
                style= background-image: url(images/barcodebkg.jpg);
                class= barcodeInput
          div
            style= clear:both;
      sql insert into contents select c3.content, c2.attribute, c2.contentType, c2.content, {_eventId}
        - from contents c
        - inner join contents c2 on c.content = c2.containerId
        -  inner join transferMaps tm
        - on c.attribute = tm.sourceAttribute
        - inner join contents c3 on tm.destinationAttribute = c3.attribute
        - where c.containerId = '{sampleRack}'
        - and c3.containerId = '{elutionRack}'
        - and tm.transferMap = '1x24RowTo6x4Row'



