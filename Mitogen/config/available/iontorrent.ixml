        vertical 
          fieldset
            legend Instructions
            ol
              li Scan in resources for proton run.
              li Enter sequencing chip ID.
              li Run proton system.
              li Enter sequence results.
              li Enter comments as necessary.
              li Submit step.
#           fieldset
#             legend Resources
#             vertical
#               passiveContainer 200 Seq Reagents
#                 #qualifyResource~ qualified
#                 #containerType~ 200 Seq Reagents
#               passiveContainer 200 Seq Solutions
#                 #qualifyResource~ qualified
#                 #containerType~ 200 Seq Solutions
#               passiveContainer Seq supplies
#                 #qualifyResource~ qualified
#                 #containerType~ Seq supplies
#               passiveContainer Proton
#                 #qualifyResource~ qualified
#                 #containerType~ Proton
          fieldset
            legend Lab Processing
            vertical
              container Sequencing Chip
                deleteQueue~ Run Proton
              container Sequence Results
