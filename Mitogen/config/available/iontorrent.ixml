        include resourceTable
        vertical 
          fieldset
            legend Instructions
            ol
              li Scan in resources for sequencing.
              li Enter library tube ID.
              li Enter sequencing chip ID.
              li Perform sample sequencing.
              li Enter comments as necessary.
              li Submit step.
#           fieldset
#             legend Resources
#             vertical
#               passiveContainer Sequencing Reagents 200
#                 #qualifyResource~ qualified
#                 #containerType~ Sequencing Reagents 200
#               passiveContainer Sequencing Solutions 200
#                 #qualifyResource~ qualified
#                 #containerType~ Sequencing Solutions 200
          fieldset
            legend Lab Processing
            vertical
              container Library Tube
                deleteQueue~ Prep Sample for Sequencing
              container Sequencing Chip
                deleteQueue~ Prep Sample for Sequencing
                insertQueue~ Run Proton
              comments Comments
      xfer duplicate
        from Library Tube
        to Sequencing Chip
     
    step Run Proton
      
      form 
        include userAccountInfo
